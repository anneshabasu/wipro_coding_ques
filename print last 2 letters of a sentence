#include<iostream>
#include<string.h>
using namespace std; 
int main()
{
    char s[100];
    int i=0, count, length;
    cout<<"Enter the string: ";
    cin.getline(s,100);
    length=strlen(s);
    for(i=0; i<=length;i++)
    {
        if(s[i]==' ' && s[i+1]!=' ' && s[i+1]!='\0')
        {
            count++;
        }
        
    }
    cout<<count<<endl;
    cout << s[length-2] <<s[length-1]; 
}
